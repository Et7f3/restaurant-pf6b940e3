<% if (rows.length != 0) { %>
    <table>
        <thead>
            <tr>
                <th>id</th><th>login</th>
            </tr>
        </thead>
        <tbody>
            <% for (let row of rows) { %>
                <tr>
                    <td><%- row.id %></td>
                    <td><%= row.login %></td>
                </tr>
            <% } %>
        </tbody>
    </table>
<% } else { %>
    <p> Aucun restaurateurs inscrits.</p>
<% } %>
<a href="/admin/signin">Retour</a>
